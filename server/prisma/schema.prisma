datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model RecipeTitle {
  id    Int     @id @default(autoincrement())
  title String
  create_datetime DateTime
  update_datetime DateTime
}

model ScaleType {
  id Int @id @default(autoincrement())
  name String
  create_datetime DateTime
  update_datetime DateTime
}

model Materials {
  id Int @id @default(autoincrement())
  name String
  scaleTypeId Int
  scale Int
}